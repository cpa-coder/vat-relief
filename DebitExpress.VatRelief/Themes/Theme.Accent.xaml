<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <Color
        x:Key="SecondaryHueLightColor">
        #FF9E40
    </Color>
    <Color
        x:Key="SecondaryHueMidColor">
        #FF6D00
    </Color>
    <Color
        x:Key="SecondaryHueDarkColor">
        #C43C00
    </Color>
    <Color
        x:Key="SecondaryLightForegroundColor">
        #FFF4E9
    </Color>
    <Color
        x:Key="SecondaryDarkForegroundColor">
        #070200
    </Color>
    <SolidColorBrush
        x:Key="SecondaryHueLightBrush"
        Color="#FF9E40"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueMidBrush"
        Color="#FF6D00"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueDarkBrush"
        Color="#C43C00"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueDarkForegroundBrush"
        Color="#FFF4E9"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueMidForegroundBrush"
        Color="#FFF4E9"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="SecondaryHueLightForegroundBrush"
        Color="#070200"
        po:Freeze="True" />
</ResourceDictionary>