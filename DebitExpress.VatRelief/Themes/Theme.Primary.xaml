<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:po="http://schemas.microsoft.com/winfx/2006/xaml/presentation/options">
    <Color
        x:Key="PrimaryHueLightColor">
        #827276
    </Color>
    <Color
        x:Key="PrimaryHueMidColor">
        #634B51
    </Color>
    <Color
        x:Key="PrimaryHueDarkColor">
        #433237
    </Color>
    <Color
        x:Key="PrimaryLightForegroundColor">
        #FFEBE8
    </Color>
    <Color
        x:Key="PrimaryDarkForegroundColor">
        #050000
    </Color>
    <SolidColorBrush
        x:Key="LightBrush"
        Color="#FFEBE8"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="PrimaryHueLightBrush"
        Color="#827276"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="PrimaryHueMidBrush"
        Color="#634B51"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="PrimaryHueDarkBrush"
        Color="#433237"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="PrimaryHueDarkForegroundBrush"
        Color="#FFEBE8"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="PrimaryHueMidForegroundBrush"
        Color="#FFEBE8"
        po:Freeze="True" />
    <SolidColorBrush
        x:Key="PrimaryHueLightForegroundBrush"
        Color="#050000"
        po:Freeze="True" />
</ResourceDictionary>